    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.2.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "prettier": "^2.1.1",
    "react": "16.13.1",
    "react-native": ">=0.60 <=0.64",
    "semantic-release": "^17.1.1",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
